{
  "name": "iomywiab/iomywiab-php-constraints",
  "type": "library",
  "description": "Generic constraints,  mostly used for parameter checking",
  "keywords": ["constraint", "validation", "check", "verification"],
  "homepage": "https://github.com/iomywiab/iomywiab-php-constraints",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "iomywiab",
      "email": "iomywiab@premium-postfach.de"
    }
  ],
  "require": {
    "php": "^7.1 || ^8.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7"
  },
  "autoload": {
    "psr-4": {
      "iomywiab\\iomywiab_php_constraints\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "iomywiab\\iomywiab_php_constraints_tests\\": "tests/"
    }
  }
}
