{
  "name": "iomywiab/iomywiab-php-constraints",
  "type": "library",
  "description": "Generic constraints,  mostly used for parameter checking",
  "keywords": ["constraint", "validation", "check", "verification"],
  "homepage": "https://github.com/iomywiab/iomywiab-php-constraints",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "iomywiab",
      "email": "iomywiab@premium-postfach.de"
    }
  ],
  "require": {
    "php": ">=8.1, <8.2"
  },
  "require-dev": {
    "phpmd/phpmd": "^2.12.0",
    "phpstan/phpstan": "^1.6.3",
    "phpstan/extension-installer": "1.1.0",
    "phpstan/phpstan-strict-rules": "^1.2.0",
    "phpunit/phpunit": "^9",
    "psalm/plugin-phpunit": "^0.16",
    "roave/security-advisories": "dev-latest",
    "squizlabs/php_codesniffer": "^3.6.2",
    "vimeo/psalm": "^4.22.0"
  },
  "autoload": {
    "psr-4": {
      "iomywiab\\iomywiab_php_constraints\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "iomywiab\\iomywiab_php_constraints_tests\\": "tests/",
      "iomywiab\\iomywiab_php_constraints_testtools\\": "testtools/"
    }
  }
}
